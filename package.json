{
  "name": "fitnesstracker",
  "version": "0.0.1",
  "description": "Fitness Tracker for personal use",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" css/style.comp.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DLCIncluded/FitnessTracker.git"
  },
  "author": "DLCIncluded",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DLCIncluded/FitnessTracker/issues"
  },
  "homepage": "https://github.com/DLCIncluded/FitnessTracker#readme",
  "devDependencies": {
    "node-sass": "^4.12.0",
    "autoprefixer": "^7.1.4",
    "postcss-cli": "^4.1.1"
  }
}